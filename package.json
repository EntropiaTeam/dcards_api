{
  "name": "printible-api-nest",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build --webpack",
    "start": "cross-env NODE_OPTIONS=--enable-source-maps nest start",
    "start:dev": "cross-env NODE_OPTIONS=--enable-source-maps nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node --enable-source-maps dist/main",
    "start:bench": "node --heap-prof --heapsnapshot-signal=SIGUSR2 --inspect=0.0.0.0:9229 --enable-source-maps dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "load-test": "k6 run --out influxdb=http://localhost:8086/resultsdb --summary-export ./test/load-tests/fullCycle-sum.json ./test/load-tests/fullCycle.js",
    "load-test:short": "k6 run ./test/load-tests/shortCycle.js",
    "load-test:two-stores": "k6 run --out influxdb=http://localhost:8086/resultsTwoStoresdb --summary-export ./test/load-tests/fullCycleTwoStores-sum.json ./test/load-tests/fullCycleTwoStores.js"
  },
  "dependencies": {
    "@azure/identity": "^1.3.0",
    "@azure/keyvault-secrets": "^4.1.0",
    "@azure/storage-blob": "^12.2.1",
    "@hapi/joi": "^17.1.1",
    "@nestjs/common": "^7.6.11",
    "@nestjs/config": "^0.5.0",
    "@nestjs/core": "^7.0.0",
    "@nestjs/platform-express": "^7.0.0",
    "@nestjs/schedule": "^0.4.1",
    "@nestjs/swagger": "^4.8.0",
    "@nestjs/typeorm": "^7.1.4",
    "@types/cron": "^1.7.2",
    "@types/crypto-js": "^4.0.1",
    "@types/pdfkit": "^0.10.6",
    "applicationinsights": "^1.8.7",
    "axios": "^0.21.1",
    "cache-manager": "^3.4.0",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "cron": "^1.8.2",
    "cross-env": "^7.0.3",
    "crypto-js": "^4.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "file-type": "^16.0.1",
    "get-stream": "^6.0.0",
    "grapheme-splitter": "^1.0.4",
    "into-stream": "^6.0.0",
    "jimp": "^0.16.1",
    "mockdate": "^3.0.2",
    "mongodb": "^3.6.2",
    "node-cache": "^5.1.2",
    "pdfkit": "^0.11.0",
    "reflect-metadata": "^0.1.13",
    "request-ip": "^2.1.3",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.4",
    "stream-mock": "^2.0.5",
    "swagger-ui-express": "^4.1.4",
    "typeorm": "^0.2.28",
    "utils-decorators": "^1.7.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.6.0",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.0",
    "@types/express": "^4.17.3",
    "@types/hapi__joi": "^17.1.6",
    "@types/jest": "26.0.10",
    "@types/mongodb": "^3.6.3",
    "@types/node": "^13.9.1",
    "@types/request-ip": "0.0.35",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.6.1",
    "copy-webpack-plugin": "^6.3.2",
    "eslint": "^7.12.1",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-nestjs": "^1.2.3",
    "husky": "^4.3.5",
    "jest": "26.4.2",
    "start-server-webpack-plugin": "^2.2.5",
    "supertest": "^4.0.2",
    "ts-jest": "26.2.0",
    "ts-loader": "^6.2.1",
    "ts-node": "9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.4",
    "webpack-node-externals": "^2.5.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "eslint ./src --max-warnings=0 --ext .ts"
    }
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}